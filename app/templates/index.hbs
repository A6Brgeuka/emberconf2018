<div class="row">
  <div class="col-md-6 col-md-offset-3">
    {{#each model as |post|}}
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title">{{post.title}} <small>{{post.date}}</small></h3>
        </div>
        <div class="panel-body">
          {{post.bodyAsHtml}}
        </div>
      </div>
    {{/each}}
  </div>
</div>